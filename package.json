{
  "name": "@ngx-gsap/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve angular-animations",
    "build": "nx build angular-animations",
    "build:prod": "nx build angular-animations --configuration=production --base-href=/ngx-gsap/",
    "deploy": "npm run build:prod && npx angular-cli-ghpages --dir=dist/angular-animations/browser --repo=https://github.com/marcos-velasquez/ngx-gsap.git",
    "test": "nx test",
    "lib:build": "nx build ngx-gsap",
    "lib:test": "nx test ngx-gsap",
    "lib:lint": "nx lint ngx-gsap",
    "lib:publish": "npm run lib:build && cd dist/packages/ngx-gsap && npm publish --access public",
    "lib:publish:dry": "npm run lib:build && cd dist/packages/ngx-gsap && npm publish --dry-run",
    "lib:link": "npm run lib:build && cd dist/packages/ngx-gsap && npm link",
    "lib:version:patch": "cd packages/ngx-gsap && npm version patch",
    "lib:version:minor": "cd packages/ngx-gsap && npm version minor",
    "lib:version:major": "cd packages/ngx-gsap && npm version major",
    "deps:validate": "depcruise packages/ngx-gsap/src",
    "deps:graph": "depcruise packages/ngx-gsap/src --output-type dot | dot -T svg > dependency-graph.svg",
    "deps:archi": "depcruise packages/ngx-gsap/src --output-type archi | dot -T svg > architecture-graph.svg"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~20.3.0",
