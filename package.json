    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.40.0",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "^20.3.0",
    "autoprefixer": "^10.4.0",
